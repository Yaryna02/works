#include <stdio.h>

int main() {
    int arr[3][3] = {{-1, -2, -3}, {-4, -5, -6}, {-7, -8, -9}}; // ініціалізуємо масив
    
    // виведемо початковий масив
    printf("Початковий масив:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }
    
    // замінимо всі елементи, менші за нуль, на одиниці
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (arr[i][j] < 0) {
                arr[i][j] = 1;
            }
        }
    }
    
    // виведемо модифікований масив
    printf("Модифікований масив:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
